{
  "version": 2,
  "headers": [
    {
      "source": "/css/(.*).css",
      "headers" : [
        {
          "key" : "X-Content-Type-Options",
          "value" : "nosniff"
        }
      ]
    },
    {
      "source": "/js/(.*).js",
      "headers" : [
        {
          "key" : "X-Content-Type-Options",
          "value" : "nosniff"
        }
      ]
    }
  ]
}
