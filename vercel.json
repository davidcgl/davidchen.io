{
  "version": 2,
  "build": {
    "env": {
      "ZOLA_VERSION": "0.12.2"
    }
  },
  "routes": [
    {
      "src": "/css/*.css",
      "headers": { "x-content-type-options": "nosniff" },
      "continue": true
    },
    {
      "src": "/js/*.js",
      "headers": { "x-content-type-options": "nosniff" },
      "continue": true
    },
    { "handle": "filesystem" },
    { "src": "/(.*)", "status": 404, "dest": "/404.html" }
  ]
}
