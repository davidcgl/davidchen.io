---
layout: base
title: Home
---

<ul class="post-list">
  {% for post in collections.posts %}
    <li>
      <h1 class="post-title">
        <a href="{{ post.url }}">{{ post.data.title }}</a>
      </h1>
      <div class="post-date">{{ post.date | date: "%B %e, %Y" }}</div>
    </li>
  {% endfor %}
</ul>
