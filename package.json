{
  "name": "davidchen.io",
  "version": "1.0.0",
  "repository": "https://github.com/davidcgl/davidchen.io.git",
  "author": "David Chen <cgl.david@gmail.com>",
  "license": "MIT",
  "private": true,
  "browserslist": [
    "defaults"
  ],
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "build": "run-s build:gulp build:eleventy",
    "build:dev": "run-s build:gulp build:eleventy:dev",
    "build:eleventy": "ELEVENTY_ENV=production eleventy",
    "build:eleventy:dev": "ELEVENTY_ENV=development eleventy",
    "build:gulp": "gulp build",
    "clean": "run-p clean:*",
    "clean:eleventy": "rm -rf dist",
    "clean:gulp": "gulp clean",
    "format": "prettier --write *.js src/**/*{css,js,json,md}",
    "watch": "run-s build:gulp && run-p watch:*",
    "watch:eleventy": "ELEVENTY_ENV=development eleventy --serve",
    "watch:gulp": "gulp watch"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@11ty/eleventy-plugin-rss": "^1.0.7",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "autoprefixer": "^10.1.0",
    "cssnano": "^4.1.10",
    "del": "^6.0.0",
    "gulp": "^4.0.2",
    "gulp-postcss": "^9.0.0",
    "gulp-real-favicon": "^0.3.2",
    "gulp-rename": "^2.0.0",
    "gulp-rev": "^9.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-size": "^3.0.0",
    "gulp-sourcemaps": "^3.0.0",
    "html-minifier": "^4.0.0",
    "instant.page": "^5.1.0",
    "lodash": "^4.17.15",
    "moment": "^2.26.0",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.2",
    "prettier": "^2.0.5",
    "rollup": "^2.11.2",
    "rollup-plugin-terser": "^7.0.2",
    "slugify": "^1.4.0"
  },
  "dependencies": {
    "fast-glob": "^3.2.2"
  }
}
